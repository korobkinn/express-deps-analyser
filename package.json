{
  "name": "express-deps-analyser",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "test:coverage": "nyc mocha -r ts-node/register tests/**/*.test.ts",
    "test:watch": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register tests/**/*.test.ts\"",
    "lint": "eslint --cache --ext .ts .",
    "lint:fix": "eslint --cache --ext .ts . --fix",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "prestart": "npm run build",
    "start": "node ./",
    "server:watch": "ts-node-dev --respawn --transpile-only --watch .env src/index.ts"
  },
  "keywords": [],
  "author": "Aleksei Korobkin",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "chai": "^4.3.7",
    "chai-json-equal": "^0.0.1",
    "eslint": "^8.34.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "follow-redirects": "^1.15.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korobkinn/express-deps-analyser.git"
  },
  "bugs": {
    "url": "https://github.com/korobkinn/express-deps-analyser/issues"
  },
  "homepage": "https://github.com/korobkinn/express-deps-analyser#readme",
  "description": ""
}
